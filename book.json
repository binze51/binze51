{
    "title": "田和明的书", 
    "description" : "记录学习，沉淀知识", 
    "author": "@ iwhile <iwhile.tian@gmail.com>",
    "authorHomepage": "http://page.pypygo.com",
    "language": "zh-hans", 
    "styles": {
        "pdf": "static/css/pdf.css"
    },
    "links": {
        "sidebar": {
            "外链站点": "http://github.com/pypygo"
        }
    },
    "pdf": {
        "fontSize": 12,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin": {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": false,
        "paperSize": "a4"
    },
    "plugins": [
        "theme-code",
        "anchor-navigation-ex",
        "page-footer-ex",
        "splitter",
        "favicon",
        "-sharing",
        "pageview-count",
        "-lunr", "-search", "search-pro",
        "flexible-alerts",
        "code",
        "github-buttons",
        "todo",
        "-fontsettings",
        "copy-code-button",
        "github",
        "timeline",
        "-highlight",
        "advanced-emoji"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
            },
        "page-footer-ex": {
            "copyright": "Copyright © 2019 [<em>pypygo</em>](https://github.com/pypygo)",
            "markdown": true,
            "update_label": "<i>updated</i>",
            "update_format": "YYYY-MM-DD HH:mm:ss"
        },
        "github": {
            "url": "https://github.com/pypygo"
        }
    }
    
}