{
      "title": "田和明的书", 
      "description" : "记录学习，沉淀知识", 
      "author": "iwhile",
      "language": "zh-hans", 
        "plugins": [
            "theme-code",
            "anchor-navigation-ex",
            "splitter",
            "prism",
            "favicon",
            "-sharing",
            "pageview-count",
            "-lunr", "-search", "search-pro",
            "flexible-alerts",
            "code",
            "tbfed-pagefooter",
            "github-buttons",
            "todo",
            "-fontsettings",
            "copy-code-button",
            "github",
            "popup",
            "prism-themes",
            "-highlight",
            "advanced-emoji"
